{
  "manifest_version": 3,
  "name": "Page Blockers",
  "description": "Block unwanted pages",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": ["<all_urls>", "http://*/*", "https://*/*"]
}
